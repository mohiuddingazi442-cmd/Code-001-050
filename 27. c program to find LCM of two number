#include <stdio.h>

// Function to find GCD using Euclid's Algorithm
int gcd(int a, int b) {
    while (b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}

int main() {
    int num1, num2, lcm;

    printf("Enter two numbers: ");
    scanf("%d %d", &num1, &num2);

    int g = gcd(num1, num2);      // Find GCD
    lcm = (num1 * num2) / g;      // LCM formula

    printf("LCM of %d and %d is %d\n", num1, num2, lcm);

    return 0;
}
